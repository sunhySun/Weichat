<!-- 卡片集测试页面 -->
<view>
  <navbar pageName="测试" />
  <view animation="{{animationData}}" style="border:1px solid gray;height:896px">
    <view>
      <progress serial="{{serial}}" total="{{total}}" />
    </view>
    <view>
      <word-choice total="{{total}}" words="{{words}}" bind:selectAns="onSelect" bind:finish="onFinish" />
    </view>
  </view>
  
</view>
